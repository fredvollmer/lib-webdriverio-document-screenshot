    "webdriverio",
    "webdrivercss",
    "webdriver",
    "plugin",
    "screenshot",
    "document"
  ],
  "private": false,
  "publishConfig": {
    "registry": "http://npm.aws.cbtnuggets.com"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-config-cbtnuggets": "^4.0.0",
    "express": "^4.14.0",
    "mocha": "^3.1.1",
    "wdio-junit-reporter": "^0.1.0",
