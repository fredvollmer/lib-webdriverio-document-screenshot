{
  "name": "lib-webdriverio-document-screenshot",
  "version": "1.0.11-dev",
  "description": "WebdriverIO plugin to save a screenshot of the entire screenshot document",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/webdriverio/bin/wdio test/wdio.conf.js",
    "lint": "./node_modules/eslint/bin/eslint.js commands/ utils/ test/wdio.conf test/specs/ --ext .js",
    "clean-bower": "rm -rf test/site/components",
    "clean-reports": "rm -rf test/reports",
    "install-bower": "cd test/site/; ../../node_modules/bower/bin/bower --allow-root install; cd ../../",
    "serve-test": "node test/helpers/express.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbtnuggets/lib-webdriverio-document-screenshot.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^0.30.0",
    "gm": "^1.23.0",
    "q": "^1.4.1",
    "temp": "^0.8.3"
  },
  "keywords": [
    "webdriverjs",
    "webdriverio",
    "webdrivercss",
    "webdriver",
    "plugin",
    "screenshot",
    "document"
  ],
  "private": false,
  "publishConfig": {
    "registry": "http://npm.aws.cbtnuggets.com"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "chai": "^3.5.0",
    "eslint": "^4.17.0",
    "eslint-config-cbtnuggets": "^7.0.0",
    "eslint-plugin-class-property": "^1.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.14.0",
    "mocha": "^3.1.1",
    "wdio-junit-reporter": "^0.1.0",
    "wdio-mocha-framework": "^0.4.3",
    "wdio-phantomjs-service": "^0.1.0",
    "webdriverio": "^4.2.16"
  }
}
